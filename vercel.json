{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html" 
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)", 
            "destination": "/api/$1"  
        }
    ]
}
