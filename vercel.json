{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "api/server.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/server.js"
      },
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ],
    "region": "sin1"
  }
  